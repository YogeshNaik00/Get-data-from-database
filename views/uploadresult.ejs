<html>
    <head>
        <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
        <style>
        </style>
    </head>
    <body>
        <div class="container">
            <form action="/upload" method="POST">
                <fieldset class="border p-2">
                    <legend  class="w-auto">Enter Student Detail and Marks</legend>
                <div class="row">
                  <div class="col"><label>Enter USN</label>
                    <input type="text" class="form-control" placeholder="Enter USN" name="usn" required>
                  </div>
                  <div class="col"><label>Enter NAME</label>
                    <input type="text" class="form-control" placeholder="Enter Name" name="name" required>
                  </div>
                </div>
        <br>
                <div class="row">
                    <div class="col"><label>KANNADA:</label>
                      <input type="text" class="form-control" name="kannada"required>
                    </div>

                    <div class="col"><label>ENGLISH:</label>
                        <input type="text" class="form-control" name="english" required>
                      </div>

                  </div>
        <br>

        <div class="row">

              <div class="col"><label>HINDI:</label>
                <input type="text" class="form-control" name="hindi" required>
              </div>

              <div class="col"><label>SCIENCE:</label>
                <input type="text" class="form-control" name="science" required>
              </div>

          </div>
          <br>

        <div class="row">
            

            <div class="col"><label>MATHS:</label>
                <input type="text" class="form-control" name="maths" required>
              </div>

              <div class="col"><label>SOCIAL:</label>
                <input type="text" class="form-control" name="social" required>
              </div>

        </div><br>
        <div>
            <button type="submit" class="btn btn-info">UPLOAD</button>
        </div>
          </form>
        </div>
    </body>
</html>